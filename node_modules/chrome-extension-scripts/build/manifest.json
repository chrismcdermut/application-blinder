{
  "manifest_version": 2,
  "homepage_url": "https://github.com/schovi/webpack-chrome-extension",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content/index.js"
      ]
    }
  ],
  "background": {
    "persistent": true,
    "scripts": [
      "background/index.js"
    ]
  },
  "page_action": {
    "default_title": "Webpack chrome extension popup action",
    "default_popup": "popup/index.html"
  },
  "chrome_url_overrides": {
    "newtab": "newtab/index.html"
  },
  "permissions": [
    "background",
    "tabs"
  ],
  "icons": {
    "16": "$assets/webpack-16.png",
    "32": "$assets/webpack-32.png",
    "128": "$assets/webpack-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost:3001; object-src 'self'; ",
  "name": "chrome-extension-scripts",
  "description": "Bootstrap playground for developing Chrome extensions with all new features like Webpack, Babel, React and almost everything you want.",
  "version": "0.9.0"
}